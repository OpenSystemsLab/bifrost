{
    "image": "mcr.microsoft.com/devcontainers/typescript-node:22",
    "features": {
      "ghcr.io/devcontainers/features/go:1": {},
      
    },
    "forwardPorts": [
      8080
    ],    
    "customizations": {
      "vscode": {
        "extensions": [
          "golang.go",
          "esbenp.prettier-vscode",
          "dbaeumer.vscode-eslint",
          "ms-vscode.vscode-typescript-tslint-plugin"
        ],
        "settings": {
          "files.watcherExclude": {
            "**/node_modules/**": true
          }
        }
      }
    }
  }